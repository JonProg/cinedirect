{
  "version": 2,
  "builds": [
      {
          "src": "server.js",
          "use": "@vercel/node"
      }
  ],
  "routes": [
      {
          "src": "/(.*)",
          "dest": "server.js"
      }
  ],
  "env": {
      "API_KEY": "0c52e48b8db6cd4f584ead1a7b99d76c",
      "API_SEARCH": "https://api.themoviedb.org/3/search/movie",
      "API_MOVIE": "https://api.themoviedb.org/3/movie/",
      "API_IMG": "https://image.tmdb.org/t/p/",
      "API_TREND": "https://api.themoviedb.org/3/trending/movie/week",
      "API_TOP20": "https://api.themoviedb.org/3/movie/top_rated?language=pt-BR&page=1",
      "API_GENRE": "https://api.themoviedb.org/3/discover/movie",
      "API_RELEASES": "https://api.themoviedb.org/3/movie/upcoming"
  }
}
